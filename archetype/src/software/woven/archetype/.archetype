TARGETS:{ime editor}

+ configuration
    - modelSequence() in "model-sequence.tsv"

    + editor
        - utilities in "utilities.txt"

- readme() in "readme.html"

+ languages in "dsls"
    - language(name) in "{name}"
    - dslFile(language) in "{language}/dsl.jar"
    - logo(language) in "{language}/logo.png"
    - readme(language) in "{language}/readme.html"
    - properties(language) in "{language}/properties.json"
    - models(language) in "{language}/models"
    - model(language, name) in "{language}/models/{name}"
    - properties(language, model) in "{language}/models/{model}/properties.json"
    - workspace(language, model) in "{language}/models/{model}/workspace"
    - releases(language, model) in "{language}/models/{model}/releases" with ".zip"
    - release(language, model, version) in "{language}/models/{model}/releases/{version}.zip"
    - releaseAccessor(language, model, version) in "{language}/models/{model}/releases/{version}-accessor.jar"

+ users in "users"
    - user(username) in "{username}"
    - photo(username) in "{username}/photo.png"
    - properties(username) in "{username}/properties.json"
    - projects(username) in "{username}/projects"
    - project(username, name) in "{username}/projects/{name}.json"

+ tmp
    - upload(name) in "uploads/{name}"
    - model(language, model) in "{language}/{model}"
    - releaseWorkspace(language, model, version) in "{language}/models/{model}/releases/{version}"